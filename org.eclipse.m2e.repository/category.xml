      <category name="m2e"/>
   </feature>
   <feature id="org.eclipse.m2e.logback.feature">
      <category name="m2e"/>
   </feature>
   <feature id="org.eclipse.m2e.lemminx.feature">
      <category name="m2e"/>
   </feature>
   <feature id="org.eclipse.m2e.pde.feature">
      <category name="m2e"/>
   </feature>
   <feature id="org.eclipse.m2e.sdk.feature">
