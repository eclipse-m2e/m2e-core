     <query> 
       <expression type="match">
         <![CDATA[
           id == $0
         ]]>
       </expression>
       <param>org.eclipse.m2e.maven.runtime</param>
     </query>
   </iu>
   <category-def name="m2e" label="Maven Integration for Eclipse"/>
